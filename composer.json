{
  "name": "jbernavaprah/eloquent-binary-cast",
  "description": "Cast correctly binary data (aka postgresql)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jure Bernava Prah",
      "email": "webjure@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/database": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.4",
    "mockery/mockery": "^1.4"
  },
  "config": {
    "discard-changes": true
  },
  "autoload": {
    "psr-4": {
      "JBernavaPrah\\EloquentBinaryCast\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JBernavaPrah\\EloquentBinaryCast\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
